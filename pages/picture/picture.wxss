.jview-bg,
.jview-select,
.jview-spirit {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 100;
}
.jview-select {
    z-index: 900;
}
.jcanvas {
    width: 100%;
    height: 100%;
}
.jview-spirit {
    border: 2rpx dashed red;
}
.jview-spirit-active {
    border: 4rpx solid red;
}